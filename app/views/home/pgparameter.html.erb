<!-- Begin page content -->
<main role="main" class="flex-shrink-0">
  <div class="container">
    <h1 class="mt-5 text-center text-black"><%=@titlepageone %></h1>
    <h4 class="mt-2 mb-4 p-2 text-light bg-info">
      <span class="glyphicon glyphicon glyphicon-file h5 text-light" aria-hidden="true"></span> <%=@titlepagetwoa %>
      <span class="glyphicon glyphicon glyphicon-chevron-right h5 text-white-50" aria-hidden="true"></span>
      <span class="glyphicon glyphicon glyphicon-cong h5 text-light" aria-hidden="true"></span> <%=@titlepagetwob %>
    </h4>
    <ul class="nav nav-tabs" id="myTab" role="tablist">
      <li class="nav-item">
        <a class="nav-link active" id="one-tab" data-toggle="tab" href="#one" role="tab" aria-controls="one" aria-selected="true">one</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" id="two-tab" data-toggle="tab" href="#two" role="tab" aria-controls="two" aria-selected="false">two</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" id="three-tab" data-toggle="tab" href="#three" role="tab" aria-controls="three" aria-selected="false">three</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" id="four-tab" data-toggle="tab" href="#four" role="tab" aria-controls="four" aria-selected="false">four</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" id="five-tab" data-toggle="tab" href="#five" role="tab" aria-controls="five" aria-selected="false">five</a>
      </li>
    </ul>
    <div class="tab-content" id="myTabContent">
      <div class="tab-pane fade show active" id="one" role="tabpanel" aria-labelledby="one-tab">
        ...one...
        <%= render partial: 'shared/icone' %>
      </div>
      <div class="tab-pane fade" id="two" role="tabpanel" aria-labelledby="two-tab">.
        ..two...
      </div>
      <div class="tab-pane fade" id="three" role="tabpanel" aria-labelledby="three-tab">
        ...three...
      </div>
      <div class="tab-pane fade" id="four" role="tabpanel" aria-labelledby="four-tab">
        ...four...
      </div>
      <div class="tab-pane fade" id="five" role="tabpanel" aria-labelledby="five-tab">
        ...five...
      </div>
    </div>
  </div>
</main>
<!-- End page content -->

